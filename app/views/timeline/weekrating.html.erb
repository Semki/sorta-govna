<div id="week-rating">
<h1> <%= t(:week_rating) %> </h1>
<table >
  <tr>
    <th><%= t(:shit) %></th>
    <th><%= t(:total) %></th>
  </tr>
<% @timeline.each do |timelineitem| %>
  <tr cols='3'>
      <td class="shit-name-cell"><%= link_to timelineitem.shit.name, shit_info_url(:shit_id => timelineitem.shit.id) %></td>
      <td><%= timelineitem.total %></td>
  </tr> 
<% end %>
</table>
</div>